{
  "name": "use-auth",
  "version": "1.2.0",
  "description": "Simple authentication composition functions for vue 3",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "./dist/use-auth.es.js",
  "exports": {
    ".": {
      "import": "./dist/use-auth.es.js",
      "require": "./dist/use-auth.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuellandreau/use-auth.git"
  },
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.25",
    "vue": "^3.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "babel-jest": "^27.5.0",
    "jest": "^27.4.7",
    "jest-mock-axios": "^4.5.0",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "vite": "^2.7.2",
    "vue-composition-test-utils": "^1.0.3",
    "vue-tsc": "^0.29.8"
  },
  "keywords": [
    "vue",
    "auth",
    "authentication",
    "composition",
    "use"
  ],
  "author": "Manuel Landreau <manuel.landreau@pm.me>",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ],
    "plugins": [
      [
        "@babel/transform-runtime",
        {
          "regenerator": true
        }
      ]
    ]
  }
}
